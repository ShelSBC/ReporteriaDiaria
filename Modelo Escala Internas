                '388S', '388Y', '454E', '454O', '454P', '454S', '454Y', '468E', '468O', '468P', '468S', '468Y', '471E', '471O', '471P', '471S', '471Y', '472E', '472O', '472P',
                '472S', '472Y', '501C', '501X', '502C', '502X', '507C', '507X', '901P', '901S', '901Y', '902P', '902S', '902X', '902Y', '903P', '903S', '903X', '903Y', '904P',
                '904S', '904X', '904Y', '905P', '905S', '905Y', '906P', '906S', '906Y', '907P', '907S', '907Y', 'D001', 'DELY', 'DILB', 'MACS', 'MCNS', '502W', '502L', '502S',
                'FPDI', '004T', 'WEB1', 'WEB2', 'WEB7', '003C', '004C', '004K', '002R', '003R', '004R', '001U', '002U', '007U', '001S', '002S', '007S')

and A.U6CYCDUE in ('2','3','4','5','6')) or (A.DMQUE in ('D001')and A.U6CYCDUE in ('0','1')))
and (A.U6BLOCKCODE1 not in ('A','K') or A.U6BLOCKCODE1 is null)
and (A.DMAGENCY is null)

group by DMQUE) P1

LEFT OUTER JOIN

(select PMQUE as FILA, sum(PMTAMT) as IMPORTE
from RCVRY.PMTFIL
where PMACCTG IN ('5','6')
and trunc(PMEDATE) >= trunc(to_date('01/07/2025','dd/mm/yyyy')) and trunc(PMEDATE) <= trunc(to_date('31/07/2025','dd/mm/yyyy'))

and PMQUE in ('001C', '001E', '001L', '001N', '001O', '001P', '001W', '001X', '001Y', '002C', '002I', '002L', '002M', '002N', '002O', '002P', '002W', '002X', '002Y', '003L',
              '003M', '003P', '003W', '003X', '003Y', '004I', '004L', '004M', '004W', '004Y', '005P', '005Y', '007C', '007N', '007O', '007P', '007X', '007Y', '010E', '010L',
              '010O', '010P', '010S', '010X', '010Y', '011E', '011L', '011O', '011P', '011S', '011X', '011Y', '014E', '014L', '014O', '014P', '014S', '014X', '014Y', '016E',
