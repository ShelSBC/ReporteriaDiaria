when ESCALA2.PLAZA = 'San Luis Potosí' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'San Miguel de Allende' then 'Zona 4 (ARS)'
when ESCALA2.PLAZA = 'Tampico' then 'Zona 5 (MAM)'
when ESCALA2.PLAZA = 'Tapachula' then 'Zona 5 (MAM)'
when ESCALA2.PLAZA = 'Tehuacán' then 'Zona 6 (HVV)'
when ESCALA2.PLAZA = 'Tepic' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'Tepic_D2D' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'Tijuana' then 'Zona 6 (HVV)'
when ESCALA2.PLAZA = 'Tlaxcala' then 'Zona 6 (HVV)'
when ESCALA2.PLAZA = 'Toluca' then 'Zona 2 (EOV)'
when ESCALA2.PLAZA = 'Torreón' then 'Zona 2 (EOV)'
when ESCALA2.PLAZA = 'Tuxpan' then 'Zona 2 (EOV)'
when ESCALA2.PLAZA = 'Tuxtepec' then 'Zona 5 (MAM)'
when ESCALA2.PLAZA = 'Tuxtla Gutiérrez' then 'Zona 5 (MAM)'
when ESCALA2.PLAZA = 'Uriangato' then 'Zona 4 (ARS)'
when ESCALA2.PLAZA = 'Veracruz' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'Villahermosa' then 'Zona 2 (EOV)'
when ESCALA2.PLAZA = 'Xalapa' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'Zacatecas' then 'Zona 3 (RAR)'
when ESCALA2.PLAZA = 'Zamora' then 'Zona 3 (RAR)'

end as ZONA

from

(select P1.FILA as FILA, P1.CUENTAS as CUENTAS, nvl(P1.SALDO,0) as IMPORTE, nvl(P2.IMPORTE,0) as PAGOS

from

(SELECT A.DMQUE as FILA, count(A.DMACCT) as CUENTAS, sum(A.DMCURBAL) as SALDO
 FROM 
(SELECT DMQUE,DMACCT,DMCURBAL,DMACCTG,DMAGENCY,U6BLOCKCODE1,U6CYCDUE FROM RCVRY.DELQUDA_BEGIN
 WHERE trunc(BEGINDT) = trunc(to_date('01/07/2025','dd/mm/yyyy'))

UNION ALL 

SELECT DMQUE,DMACCT,DMCURBAL,DMACCTG,DMAGENCY,U5BLOCKCODE1,U5CYCDUE FROM RCVRY.DELQUDA_BEGIN_MP
WHERE trunc(BEGINDT) = trunc(to_date('01/07/2025','dd/mm/yyyy')))A

where A.DMACCTG IN ('6','5')  
and(( A.DMQUE in ('001C', '001E', '001L', '001N', '001O', '001P', '001W', '001X', '001Y', '002C', '002I', '002L', '002M', '002N', '002O', '002P', '002W', '002X', '002Y', '003L',
