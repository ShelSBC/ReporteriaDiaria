from

(select distinct(DMQUE) as FILA
 FROM 
(SELECT DMQUE,U6BLOCKCODE1,DMACCTG,U6CYCDUE,DMAGENCY FROM RCVRY.DELQUDA_BEGIN
 WHERE trunc(BEGINDT) = trunc(to_date('01/07/2025','dd/mm/yyyy'))

UNION ALL 

SELECT DMQUE,U5BLOCKCODE1,DMACCTG,U5CYCDUE,DMAGENCY FROM RCVRY.DELQUDA_BEGIN_MP
WHERE trunc(BEGINDT) = trunc(to_date('01/07/2025','dd/mm/yyyy')))A

where DMACCTG  IN ('6','5') 
