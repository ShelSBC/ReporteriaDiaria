              '310Y', '311E', '311L', '311O', '311P', '311S', '311W', '311X', '311Y', '312E', '312L', '312O', '312P', '312S', '312W', '312X', '312Y', '388E', '388O', '388P',
              '388S', '388Y', '454E', '454O', '454P', '454S', '454Y', '468E', '468O', '468P', '468S', '468Y', '471E', '471O', '471P', '471S', '471Y', '472E', '472O', '472P',
              '472S', '472Y', '501C', '501X', '502C', '502X', '507C', '507X', '901P', '901S', '901Y', '902P', '902S', '902X', '902Y', '903P', '903S', '903X', '903Y', '904P',
              '904S', '904X', '904Y', '905P', '905S', '905Y', '906P', '906S', '906Y', '907P', '907S', '907Y', 'D001', 'DELY', 'DILB', 'MACS', 'MCNS', '502W', '502L', '502S',
              'FPDI', '004T', 'WEB1', 'WEB2', 'WEB7', '003C', '004C', '004K', '002R', '003R', '004R', '001U', '002U', '007U', '001S', '002S', '007S')

group by PMQUE) P2

ON P1.FILA = P2.FILA

ORDER BY P1.FILA) ESCALA1

LEFT OUTER JOIN

(select P1.FILA as FILA, case
when (P1.FILA) like '%MACS%' then 'MS'
when (P1.FILA) like '%DELY%' then 'DY'
when (P1.FILA) like '%DILB%' then 'DB'
when (P1.FILA) like '%MCNS%' then 'MN'
when (P1.FILA) like '%MCCC%' then 'MC'
when (P1.FILA) like '%SD' then 'S'
when (P1.FILA) like '%SM' then 'S'
when (P1.FILA) like '%N' then 'N'
when (P1.FILA) like '%R' then 'R'
when (P1.FILA) like '%S' then 'S'
when (P1.FILA) like '%U' then 'U'
when (P1.FILA) like '%O' then 'O'
when (P1.FILA) like '%T' then 'T'
when (P1.FILA) like '%D001%' then 'D'
when (P1.FILA) like '%FPDI%' then 'F'
when (P1.FILA) like '%WEB%' then 'B'
when (P1.FILA) like '%TG' then 'G'
when substr(P1.FILA,4,1) = 'M' then 'M'
when substr(P1.FILA,4,1) = 'I' then 'I'
else substr(P1.FILA,4,1)
end as TIPO, 

case

when P1.FILA in ('027E','027O','027P','027S','027Y','306O','306P','306Y','306S') then 'Acapulco'
when P1.FILA in ('058E','058O','058P','058S','058Y') then 'Aguascalientes'
when P1.FILA in ('102E','102O','102P','102S','102Y') then 'Campeche'
when P1.FILA in ('075E','075O','075P','075S','075Y') then 'Cancún'
when P1.FILA in ('105E','105O','105P','105S','105Y') then 'Celaya'
when P1.FILA in ('079E','079O','079P','079S','079Y') then 'Chetumal'
when P1.FILA in ('042E','042O','042P','042S','042Y') then 'Chihuahua'
when P1.FILA in ('303E','303O','303P','303S','303Y') then 'Chilpancingo'
when P1.FILA in ('304E','304O','304P','304S','304Y') then 'Ciudad del Carmen'
when P1.FILA in ('035E','035O','035P','035S','035Y') then 'Ciudad Juárez'
when P1.FILA in ('060E','060O','060P','060S','060Y') then 'Ciudad Obregón'
when P1.FILA in ('126E','126O','126P','126S','126Y') then 'Ciudad Victoria'
when P1.FILA in ('016E','016O','016P','016S','016Y','085O','085P','085Y') then 'Coatzacoalcos'
when P1.FILA in ('084E','084O','084P','084S','084Y') then 'Colima'
when P1.FILA in ('472E','472O','472P','472S','472Y') then 'Comitán'
when P1.FILA in ('011E','011O','011P','011S','011Y') then 'Córdoba'
when P1.FILA in ('DELY','DILB','MACS','D001','002M','003M','004M','004T') then 'Metropolitano 1'
when P1.FILA in ('192E','192O','192P','192S','192Y') then 'Cuautla'
when P1.FILA in ('077E','077O','077P','077S','077Y','301Y') then 'Cuernavaca'
when P1.FILA in ('046E','046O','046P','046S','046Y') then 'Culiacán'
when P1.FILA in ('089E','089O','089P','089S','089Y') then 'Durango'
when P1.FILA in ('471E','471O','471P','471S','471Y') then 'Fresnillo'
when P1.FILA in ('051E','051O','051P','051S','051Y') then 'Guadalajara'
when P1.FILA in ('454E','454O','454P','454S','454Y') then 'Guanajuato'
when P1.FILA in ('050E','050O','050P','050S','050Y') then 'Hermosillo'
when P1.FILA in ('047E','047O','047P','047S','047Y') then 'Irapuato'
when P1.FILA in ('310E','310O','310P','310S','310Y') then 'Istmo'
when P1.FILA in ('308E','308O','308P','308S','308Y') then 'La Paz'
when P1.FILA in ('071E','071O','071P','071S','071Y') then 'León'
when P1.FILA in ('305E','305O','305P','305S','305Y') then 'Los Mochis'
when P1.FILA in ('055E','055O','055P','055S','055Y') then 'Mazatlán'
when P1.FILA in ('076E','076O','076P','076S','076Y') then 'Mérida'
when P1.FILA in ('001E','002I','002L','002W','003L','003W','004I','004L','004W','03SD','04SD','04SE',
                 '04LB','04MP','06SD','MCNS','04TG','902X','903X','904X','MCCC','D001','002M','003M',
                 '004M','FPDI','003C','004C','004K','002R','003R','004R') then 'Metropolitano 1'
when P1.FILA in ('001C','001P','001X','001Y','002C','002P','002X','005Y','005P','002Y','501C','501X',
                 '502C','502X','01SD','02SD','05SD','901Y','901P','901S','902Y','902P','902S','903Y',
                 '903P','903S','904Y','904P','904S','905Y','905P','905S','906Y','906P','906S','007C',
                 '507C','007P','007Y','07SD','007X','507X','01SC','02SC','07SC','01SM','02SM','07SM',
                 '907P','907S','907Y','001N','002N','007N','001O','002O','007O','502W','502L','502S',
                 'WEB1','WEB2','WEB7','001S','002S','007S','001U','002U','007U') then 'Metropolitano 2'
when P1.FILA in ('312E','312O','312P','312S','312Y') then 'Mexicali'
when P1.FILA in ('468E','468O','468P','468S','468Y') then 'Monclova'
when P1.FILA in ('031E','031O','031P','031S','031Y') then 'Monterrey'
when P1.FILA in ('057E','057O','057P','057S','057Y') then 'Morelia'
when P1.FILA in ('037E','037O','037P','037S','037Y') then 'Oaxaca'
